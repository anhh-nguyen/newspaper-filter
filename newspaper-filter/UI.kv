WindowManager:
    MainWindow:
    SecondWindow:

<MainWindow>:
    name: "main"
    Image:
        source: '../images/landing-page.png'
        allow_stretch: True
        size_hint_x: 1.3
        size_hint_y: 1
        pos_hint: {'center_x':.5, 'center_y':.5}
        
        TextInput:
            id: passw
            size: 150, 40
            pos: 125,320
            
        Button:
            text: "Search"
            size: 70, 40
            pos: 170,270
            opacity: 0.7
            background_color: 'black'
            on_release:
                app.root.current = "second"
                root.manager.transition.direction = "left"
            

<SecondWindow>:
    name: "second"
    Button:
        text: "Go Back"
        on_release:
            app.root.current = "main"
            root.manager.transition.direction = "right"
